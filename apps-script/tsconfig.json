{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "isolatedModules": false,
    "lib": ["ES2020"],
    "module": "None",
    "skipLibCheck": true,
    "target": "ES2020"
  },
  "include": ["*.ts"]
}
